{
    "version": "1.0",
    "name": "my/application",
    "resources": {
        "/app": "res"
    },
    "bindings": {
        "e736d4e9-a290-50f7-a15c-c30d5e4baac2": {
            "query": "/app/config*.yml",
            "type": "my/type"
        }
    },
    "binding-types": {
        "my/type": {
            "description": "Description of my type.",
            "parameters": {
                "param": {
                    "description": "Description of the parameter.",
                    "default": 1234
                }
            }
        }
    },
    "override": "acme/blog",
    "override-order": [
        "acme/blog-extension1",
        "acme/blog-extension2"
    ],
    "config": {
        "puli-dir": "puli-dir",
        "factory": {
            "class": "Puli\\MyFactory",
            "file": "{$puli-dir}/MyFactory.php"
        },
        "repository": {
            "type": "my-type",
            "path": "{$puli-dir}/my-repo"
        },
        "discovery": {
            "store": {
                "type": "my-store-type"
            }
        }
    },
    "plugins": [
        "Puli\\RepositoryManager\\Tests\\Api\\Package\\Fixtures\\TestPlugin"
    ],
    "packages": {
        "vendor/package1": {
            "install-path": "/path/to/package1",
            "installer": "composer",
            "enabled-bindings": [
                "a54e5668-2b36-43f4-a32c-2d175092b77d"
            ],
            "disabled-bindings": [
                "4d02ee67-d845-4789-a9c1-8301351c6f5a"
            ]
        },
        "vendor/package2": {
            "install-path": "/path/to/package2"
        }
    }
}
